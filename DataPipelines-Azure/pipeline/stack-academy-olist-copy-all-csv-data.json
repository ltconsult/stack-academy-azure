{
	"name": "stack-academy-olist-copy-all-csv-data",
	"properties": {
		"activities": [
			{
				"name": "ForEach_3gj",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.cw_items",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Copy_3gj",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [
								{
									"name": "Source",
									"value": "dbo.@{item().source.table}"
								},
								{
									"name": "Destination",
									"value": "landing//@{item().destination.fileName}"
								}
							],
							"typeProperties": {
								"source": {
									"type": "AzureSqlSource",
									"partitionOption": "None"
								},
								"sink": {
									"type": "DelimitedTextSink",
									"storeSettings": {
										"type": "AzureBlobFSWriteSettings"
									},
									"formatSettings": {
										"type": "DelimitedTextWriteSettings",
										"quoteAllText": true,
										"fileExtension": ".txt"
									}
								},
								"enableStaging": false,
								"validateDataConsistency": false,
								"translator": {
									"type": "TabularTranslator",
									"typeConversion": true,
									"typeConversionSettings": {
										"allowDataTruncation": true,
										"treatBooleanAsNumber": false
									}
								}
							},
							"inputs": [
								{
									"referenceName": "SourceDataset_3gj",
									"type": "DatasetReference",
									"parameters": {
										"cw_table": "@item().source.table"
									}
								}
							],
							"outputs": [
								{
									"referenceName": "DestinationDataset_3gj",
									"type": "DatasetReference",
									"parameters": {
										"cw_fileName": "@item().destination.fileName"
									}
								}
							]
						}
					]
				}
			},
			{
				"name": "stack-academy-olist-notebook",
				"type": "DatabricksNotebook",
				"dependsOn": [
					{
						"activity": "ForEach_3gj",
						"dependencyConditions": [
							"Skipped"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebookPath": "/Users/luciano.teixeira@ltconsult.info/olist_processing_new"
				},
				"linkedServiceName": {
					"referenceName": "stack_academy_olist_notebook",
					"type": "LinkedServiceReference"
				}
			}
		],
		"parameters": {
			"cw_items": {
				"type": "Array",
				"defaultValue": [
					{
						"source": {
							"table": "olist_customers_dataset"
						},
						"destination": {
							"fileName": "dbo.olist_customers_dataset.csv"
						}
					},
					{
						"source": {
							"table": "olist_geolocation_dataset"
						},
						"destination": {
							"fileName": "dbo.olist_geolocation_dataset.csv"
						}
					},
					{
						"source": {
							"table": "olist_order_items_dataset"
						},
						"destination": {
							"fileName": "dbo.olist_order_items_dataset.csv"
						}
					},
					{
						"source": {
							"table": "olist_order_payments_dataset"
						},
						"destination": {
							"fileName": "dbo.olist_order_payments_dataset.csv"
						}
					},
					{
						"source": {
							"table": "olist_order_reviews_dataset"
						},
						"destination": {
							"fileName": "dbo.olist_order_reviews_dataset.csv"
						}
					},
					{
						"source": {
							"table": "olist_orders_dataset"
						},
						"destination": {
							"fileName": "dbo.olist_orders_dataset.csv"
						}
					},
					{
						"source": {
							"table": "olist_products_dataset"
						},
						"destination": {
							"fileName": "dbo.olist_products_dataset.csv"
						}
					},
					{
						"source": {
							"table": "olist_sellers_dataset"
						},
						"destination": {
							"fileName": "dbo.olist_sellers_dataset.csv"
						}
					},
					{
						"source": {
							"table": "product_category_name_translation"
						},
						"destination": {
							"fileName": "dbo.product_category_name_translation.csv"
						}
					}
				]
			}
		},
		"annotations": []
	}
}